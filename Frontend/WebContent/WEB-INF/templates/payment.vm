#@page()<table border="0" cellpadding="0" cellspacing="0" align="center">	<tr>		<td>			<table border="0" cellpadding="0" cellspacing="0"				style="background: url(${staticRoot}/images/left_bg.gif)">				<tr>					<td><img src="${staticRoot}/images/left_left.gif" width="21"						height="29" border="0" alt=""></td>					<td><img src="${staticRoot}/images/spacer.gif" width="7"						height="1" border="0" alt=""></td>					<td width="404"><div class="lb"></div>						<div class="lw">Enter Your Payment Info</div></td>					<td><img src="${staticRoot}/images/left_right.gif" width="6"						height="29" border="0" alt=""></td>				</tr>			</table>			<table border="0" cellpadding="0" cellspacing="0">				<tr>					<td style="background: url(${staticRoot}/images/c_left.gif)"><img						src="${staticRoot}/images/spacer.gif" width="1" height="1"						border="0" alt=""></td>					<td width="428" align="center">						<div>							<img src="${staticRoot}/images/spacer.gif" width="1" height="12"								border="0" alt="">						</div>						<form method="POST">							#springFormHiddenInput("payment.creditCard.id" "")							<table border="0" cellpadding="0" cellspacing="0" align="center">								<tr>									<td>Owner</td>									<td>#springFormInput("payment.creditCard.owner"										"maxlength=50") #springShowErrors("<br>" "")									</td>								</tr>								<tr>									<td>Credit Card Type</td>									<td>										#springFormSingleSelect("payment.creditCard.creditCardType"										${creditCardTypes} "") #springShowErrors("<br>" "")									</td>								</tr>								<tr>									<td>Credit Card Number</td>									<td>#springFormInput("payment.creditCard.cardNumber"										"maxlength=16") #springShowErrors("<br>" "")<br>									</td>								</tr>								<tr>									<td>CVC</td>									<td>#springFormInput("payment.creditCard.cvc" "maxlength=3										style='width:40px'") #springShowErrors("<br>" "")<br>									</td>								</tr>								<tr>									<td>Date</td>									<td>#springFormSingleSelect("payment.creditCard.month"										${months} "") #springShowErrors("<br />" "")										#springFormSingleSelect("payment.creditCard.year" ${years}										"style='margin-left:10px;'") #springShowErrors("<br />" "")									</td>								</tr>								<tr>									<td colspan="2">										<div id="availablePlans"></div>									</td>								</tr>								#if( ${showSaveCheck} )								<tr>									<td><input type="checkbox" name="saveCardInfo" value="1" />										Save Credit Card Info</td>								</tr>								#end								<tr>									<td>										<center>											<input type="image" src="${staticRoot}/images/but_buy.gif"												width="55" height="23" /><br/><br/>										 #if(  ${errorOccured} )												<span style="color:red;font-size:13px;">${errorMessage}</span></br>												<span style="color:red;font-size:15px;">${errorMessageDetail}</span>										  #end										  </center>									</td>								</tr>									</td>								</tr>								<tr>									<td colspan="2">&nbsp</td>								</tr>								<tr>									<td colspan="2">&nbsp</td>								</tr>								<tr>									<td colspan="2">&nbsp</td>								</tr>								<tr>									<td colspan="2">&nbsp</td>								</tr>								#parse("installmentplan.vm")							</table>						</form>						<div>							<img src="${staticRoot}/images/spacer.gif" width="1" height="14"								border="0" alt="">						</div>					</td>					<td style="background: url(${staticRoot}/images/c_right.gif)"><img						src="${staticRoot}/images/spacer.gif" width="1" height="1"						border="0" alt=""></td>				</tr>				<tr>					<td><img src="${staticRoot}/images/c4.gif" width="5"						height="5" border="0" alt=""></td>					<td style="background: url(${staticRoot}/images/c_bot.gif)"><img						src="${staticRoot}/images/spacer.gif" width="1" height="1"						border="0" alt=""></td>					<td><img src="${staticRoot}/images/c3.gif" width="5"						height="5" border="0" alt=""></td>				</tr>			</table>		</td>	</tr></table><script type="text/javascript">	$(document).ready(function() {		var service = new PaymentService();		var view = new PaymentView();		var controller = new PaymentController(view, service);		controller.start();	});</script>#end