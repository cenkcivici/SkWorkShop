#if( $orderRejectionMessage )	<span style="color:red;font-size:14px;">$orderRejectionMessage</span>#end#if ($orders.size() > 0)<table border="0" cellpadding="5" cellspacing="5">	<tr>		<td>Order Date</td>		<td>Order Status</td>		<td>Order Amount</td>		<td>Orders</td>		<td>Reject Order?</td>			</tr>	#foreach($order in $orders)	<tr>		<td>$order.orderDate</td>		<td>$order.orderStatus</td>		<td>$order.totalAmount</td>		<td style="border:1px solid grey">		#if ($order.shoppingCart.items.size() > 0)			<table border="0" cellpadding="5" cellspacing="5">				<tr>					<td>Product Name</td>					<td>Price</td>					<td>Quantity</td>					<td>Cost</td>				</tr>				#foreach($item in $order.shoppingCart.items)				<tr>					<td>$item.product.title</td>					<td>$item.product.price</td>					<td>$item.quantity</td>					<td>$item.cost</td>				</tr>				#end			</table>		#end		</td>		<td>			#if ( $order.underReview)			<img class="rejectOrder" style="cursor:pointer"			src="${staticRoot}/images/delete_icon.png" id="$order.id" />			#end					</td>	</tr>	#end</table>#else	No previous orders found#end